{
  "name": "sql-index-predictor",
  "version": "1.0.10",
  "description": "A module that allows you to automatically predict the indexes that need to be created based on queries.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/index.ts"
  },
  "keywords": [
    "index",
    "sql",
    "suggestion",
    "prediction"
  ],
  "author": "Arthur Lazarenko",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.21",
    "pgsql-ast-parser": "11.0.1",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@types/lodash": "4.14.195",
    "nodemon": "2.0.22",
    "ts-node": "10.9.1"
  }
}
